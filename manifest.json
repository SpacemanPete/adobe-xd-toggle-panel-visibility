{
  "name": "Toggle Sidenav Visibility for Adobe XD",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Allows user to toggle the Sidenav visibility for Adobe XD pages",
  "homepage_url": "https://github.com/SpacemanPete/adobe-xd-toggle-sidenav-visibility",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://xd.adobe.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://xd.adobe.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}